{
  "expo": {
    "name": "statify",
    "slug": "statify",
    "scheme": "statify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/spotify.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFD4CF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.nujabness.statify",
      "versionCode": 1
    },
    "web": {
      "favicon": "./src/assets/spotify.ico"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "json",
        "ts",
        "tsx",
        "vue"
      ]
    }
  }
}